<html>
<head>
<title>TipMap demo</title>
</head>
<body>
<div class="tipmap">
<div class="tipmap__inner">
<% images.forEach(function (src, index) { %>
<img class="tipmap__image" src="<%= src %>" alt="image-<%= index %>"/><% }) %>
<!-- /.tipmap__inner --></div>
<!-- /.tipmap --></div>
<style>
.tipmap {
  position: absolute;
  max-width: 100%;
  max-height: 100%;
  overflow: hidden;
}
.tipmap__inner {
  position: relative;
  width: <%= fullWidth %>px;
  height: <%= fullHeight %>px;
}

.tipmap__image {
  display: block;
  float: left;
  margin: 0px;
}
</style>
</body>
</html>
